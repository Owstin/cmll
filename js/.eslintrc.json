{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "angular",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": "off",
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": "off",
        "angular/angularelement": "off"
    }
}